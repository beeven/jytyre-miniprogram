/**index.wxss**/


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  height: 100%;
}

.container {
  display: flex;
  flex-direction: column;
  height: 100%;
  background-image: url(data:image/jpeg;base64,);

  background-size: cover;
}


.banner {
  color: white;
  margin: 20rpx;
}

.logo {
  width: 100%;
  flex: 1 1;
  justify-content: center;
  align-items: center;
  display: flex;
}

.logo image {
  width: 100%; 
  height: 440rpx;
  max-width: 750rpx;
}

.statistics {
  color: white;
  text-align: center;
  height: 200rpx;
  line-height: 100rpx;
}

.buttons {
  height: 350rpx;
  width: 100%;
}

.row {
  display: flex;
  justify-content: stretch;
  align-items: stretch;
  padding-left: 30rpx;
  padding-right: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}



.row navigator,
.row button {
  flex: 1 1;
  display: flex;
  font-size: 1rem;
  border-radius: 0;
  color: white;
  align-items: center;
  justify-content: center;
  height: 130rpx;
  padding: 0;
  margin: 0;
  /* background: rgb(30,29,43);
  border-radius: 20rpx;
  margin: 10rpx; */
}

.row:first-of-type {
  border-bottom: 3rpx solid rgba(234,30,99, 0.5);
}

.row button:first-child,
.row navigator:first-child {
  border-right: 3rpx solid rgba(234,30,99,0.5);
}

.row text {
  display: block;
}

.iconfont {
  font-size: 1.3rem;
  margin-right: 0.4rem;
  color: rgba(245, 32, 32, 0.7);
}
